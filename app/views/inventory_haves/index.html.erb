<h1>Listing inventory_haves</h1>

<table>
  <thead>
    <tr>
      <th>Book</th>
      <th>User</th>
      <th>Condition</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_haves.each do |inventory_hafe| %>
      <tr>
        <td><%= inventory_hafe.book_id %></td>
        <td><%= inventory_hafe.user_id %></td>
        <td><%= inventory_hafe.condition_id %></td>
        <td><%= link_to 'Show', inventory_hafe %></td>
